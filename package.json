{
  "name": "arvinxx-api",
  "version": "1.0.0",
  "private": true,
  "description": "空谷自用的 API",
  "repository": "https://github.com/arvinxx/api.git",
  "scripts": {
    "start": "vercel dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "-------     ": "",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./api && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./api",
    "lint:prettier": "prettier --check \"api/**/*\" --end-of-line auto",
    "prettier": "prettier -c --write \"api/**/*\"",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "umi-request": "^1.3.5",
    "zotero-api-client": "^0.35.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.144",
    "@umijs/test": "^3.3.9",
    "@umijs/fabric": "^2.5.7",
    "@vercel/node": "^1.9.0",
    "chalk": "^4.0.0",
    "commitlint": "^12.0.1",
    "commitlint-config-gitmoji": "^2.2.3",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.1",
    "semantic-release": "^17.4.1",
    "semantic-release-config-gitmoji": "^1.2.1",
    "stylelint": "^13.12.0",
    "typescript": "^4.2.3",
    "vercel": "^21.3.2"
  }
}
