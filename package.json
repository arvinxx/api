{
  "name": "arvinxx-api",
  "version": "1.0.0",
  "private": true,
  "description": "空谷自用的 API",
  "repository": "https://github.com/arvinxx/api.git",
  "scripts": {
    "start": "vercel dev",
    "build": "umi build",
    "test": "umi test",
    "-------     ": "",
    "lint": "umi g tmp && npm run lint:js && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./api && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./api",
    "lint:prettier": "prettier --check \"api/**/*\" --end-of-line auto",
    "prettier": "prettier -c --write \"api/**/*\"",
    "tsc": "tsc --noEmit",
    "-------      ": "",
    "postinstall": "umi g tmp"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "react": "^17.0.0",
    "umi": "^3.3.9",
    "zotero-api-client": "^0.34.2",
    "umi-request": "^1.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^2.5.7",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/test": "^3.3.9",
    "@umijs/yorkie": "^2.0.3",
    "chalk": "^4.0.0",
    "commitlint": "^11.0.0",
    "commitlint-config-gitmoji": "^2.2.2",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.1",
    "semantic-release": "^17.4.0",
    "semantic-release-config-gitmoji-module": "^1.1.1",
    "stylelint": "^13.0.0",
    "typescript": "^4.2.2",
    "vercel": "^21.3.2"
  }
}
